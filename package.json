{
  "name": "sf-commit-studio",
  "displayName": "SF Commit Studio",
  "description": "Visual Salesforce metadata commit tool",
  "icon": "resources/icon.png",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/example/sf-commit-studio.git"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sfCommitStudio.open",
        "title": "SF Commit Studio: Open"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "mocha --require ts-node/register --experimental-specifier-resolution=node src/test/**/*.test.ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.x",
    "@types/sinon": "^21.0.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "sinon": "^21.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@evan-hyer/track-changes": "^1.0.0"
  }
}
